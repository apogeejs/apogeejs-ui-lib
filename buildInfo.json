{
    "repoName": "apogeejs-ui-lib",

    "esModule": {
        "taskList": [
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-ui-lib/versionInfo.json"
                ]
            },
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-ui-lib/resources/**/*"
                ],
                "childFolder": "resources"
            },
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-ui-lib/style/**/*"
                ],
                "childFolder": "style"
            },
            {
                "type": "packageLib",
                "esModuleFileName": "apogeeUiLib.js",
                "externalLibs": [
                    "/apogeejs-base-lib/src/apogeeBaseLib.js",
                    "/apogeejs-util-lib/src/apogeeUtilLib.js"
                ]
            }
        ]
    }
}